
module
{
    interface Elastos.Utility.ICollection;

    namespace Elastos {
    namespace Security {
    namespace Cert {

    /**
     * @Involve
     * interface ICertStoreParameters
     */
    interface ICollectionCertStoreParameters {
        /**
         * Returns the collection where the {@code Certificate}s and {@code CRL}s
         * are retrieved from.
         *
         * @return the collection where the {@code Certificate}s and {@code CRL}s
         *         will be retrieved from.
         */
        GetCollection(
            [out] ICollection** collection);

        /**
         * Returns the string representation of this instance.
         *
         * @return the string representation of this instance.
         */
        ToString(
            [out] String* str);
    }

    } // namespace Cert
    } // namespace Security
    } // namespace Elastos
}
