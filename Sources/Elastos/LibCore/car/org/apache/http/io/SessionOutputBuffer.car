
module
{

    interface Org.Apache.Http.IO.IHttpTransportMetrics;
    interface Org.Apache.Http.Utility.ICharArrayBuffer;

    namespace Org {
    namespace Apache {
    namespace Http {
    namespace IO {

    /**
     * Session output buffer for blocking connections.
     *
     * @author <a href="mailto:oleg at ural.ru">Oleg Kalnichevski</a>
     *
     * @version $Revision: 560528 $
     *
     * @since 4.0
     */
    interface ISessionOutputBuffer {

        Write(
            [in] ArrayOf<Byte>* b,
            [in] Int32 off,
            [in] Int32 len);

        Write(
            [in] ArrayOf<Byte>* b);

        Write(
            [in] Int32 b);

        WriteLine(
            [in] String s);

        WriteLine(
            [in] ICharArrayBuffer* buffer);

        Flush();

        GetMetrics(
            [out] IHttpTransportMetrics** metrics);
    }

    } // namespace IO
    } // namespace Http
    } // namespace Apache
    } // namespace Org

}
