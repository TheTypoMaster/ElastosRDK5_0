
module
{

    interface Elastos.Droid.Content.Res.IConfiguration;

    namespace Elastos {
    namespace Droid {
    namespace View {
    namespace Menu {

        interface IActionMenuPresenterSavedState {

            SetOpenSubMenuId(
                [in] Int32 menuId);

            GetOpenSubMenuId(
                [out] Int32* menuId);
        }

        /**
         * @Involve
         * interface IBaseMenuPresenter
         */
        [local]
        interface IActionMenuPresenter {
            OnConfigurationChanged(
                [in] IConfiguration* newConfig);

            SetWidthLimit(
                [in] Int32 width,
                [in] Boolean strict);

            SetReserveOverflow(
                [in] Boolean reserveOverflow);

            SetItemLimit(
                [in] Int32 itemCount);

            SetExpandedActionViewsExclusive(
                [in] Boolean isExclusive);

            ShowOverflowMenu(
                [out] Boolean* res);

            HideOverflowMenu(
                [out] Boolean* res);

            DismissPopupMenus(
                [out] Boolean* res);

            HideSubMenus(
                [out] Boolean* res);

            IsOverflowMenuShowing(
                [out] Boolean* res);

            IsOverflowReserved(
                [out] Boolean* res);
        }

    } // namespace Menu
    } // namespace View
    } // namepsace Droid
    } // namespace Elastos
}
