
module {

    namespace Elastos {
    namespace Droid {
    namespace View {
    namespace Animation {

    /**
     * Static utility class for constructing native interpolators to keep the
     * JNI simpler
     */
    [deprecated]
    interface INativeInterpolatorFactoryHelper {
        CreateAccelerateDecelerateInterpolator(
            [out] Int64* result);

        CreateAccelerateInterpolator(
            [in] Float factor,
            [out] Int64* result);

        CreateAnticipateInterpolator(
            [in] Float tension,
            [out] Int64* result);

        CreateAnticipateOvershootInterpolator(
            [in] Float tension,
            [out] Int64* result);

        CreateBounceInterpolator(
            [out] Int64* result);

        CreateCycleInterpolator(
            [in] Float cycles,
            [out] Int64* result);

        CreateDecelerateInterpolator(
            [in] Float factor,
            [out] Int64* result);

        CreateLinearInterpolator(
            [out] Int64* result);

        CreateOvershootInterpolator(
            [in] Float tension,
            [out] Int64* result);

        CreateLutInterpolator(
            [in] ArrayOf<Float>* values,
            [out] Int64* result);
    }

    }   //namespace Animation
    }   //namespace View
    }   //namespace Droid
    }   //namespace Elastos
}
