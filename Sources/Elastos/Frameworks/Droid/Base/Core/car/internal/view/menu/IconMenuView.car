
module
{

    namespace Elastos {
    namespace Droid {
    namespace Internal {
    namespace View {
    namespace Menu {

        /**
         * @Involve
         * interface Elastos.Droid.View.IViewBaseSavedState
         */
        interface IIconMenuViewSavedState{

            SetFocusedPosition(
                [in] Int32 focusedPosition);

            GetFocusedPosition(
                [out] Int32* focusedPosition);
        }

        /**
         * @Involve
         * interface Elastos.Droid.View.IViewGroup
         */
        [local]
        interface IIconMenuView {
            /**
             * Returns the number of items per row.
             * <p>
             * This should only be used for testing.
             *
             * @return The length of the array is the number of rows. A value at a
             *         position is the number of items in that row.
             * @hide
             */
            GetLayout(
                [out, callee] ArrayOf<Int32>* items);

            /**
             * Returns the number of rows in the layout.
             * <p>
             * This should only be used for testing.
             *
             * @return The length of the array is the number of rows. A value at a
             *         position is the number of items in that row.
             * @hide
             */
            GetLayoutNumRows(
                [out] Int32* nunmber);
        }

        /**
         * @Involve
         * interface Elastos.Droid.View.IViewGroupMarginLayoutParams
         */
        interface IIconMenuViewLayoutParams {

            SetLeft(
                [in] Int32 left);

            GetLeft(
                [out] Int32* left);

            SetTop(
                [in] Int32 top);

            GetTop(
                [out] Int32* top);

            SetRight(
                [in] Int32 right);

            GetRight(
                [out] Int32* right);

            SetBottom(
                [in] Int32 bottom);

            GetBottom(
                [out] Int32* bottom);

            SetDesiredWidth(
                [in] Int32 desiredWidth);

            GetDesiredWidth(
                [out] Int32* desiredWidth);

            SetMaxNumItemsOnRow(
                [in] Int32 maxNumItemsOnRow);

            GetMaxNumItemsOnRow(
                [out] Int32* maxNumItemsOnRow);

        }

    } // namespace Menu
    } // namespace View
    } // namespace Internal
    } // namepsace Droid
    } // namespace Elastos
}
