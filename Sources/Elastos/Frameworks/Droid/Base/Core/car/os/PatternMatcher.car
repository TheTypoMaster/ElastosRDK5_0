module
{

    namespace Elastos {
    namespace Droid {
    namespace Os {

    interface IPatternMatcher {
        const Int32 PATTERN_LITERAL = 0;
        const Int32 PATTERN_PREFIX = 1;
        const Int32 PATTERN_SIMPLE_GLOB = 2;

        GetPath(
            [out] String* path);

        GetType(
            [out] Int32* type);

        Match(
            [in] String str,
            [out] Boolean* isMatch);

        ToString(
            [out] String* str);
    }

    } // namespace Os
    } // namespace Droid
    } // namespace Elastos
}
