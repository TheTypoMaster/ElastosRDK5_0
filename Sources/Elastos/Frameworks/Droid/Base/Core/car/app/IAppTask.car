    interface Elastos.Droid.Os.IBinder;
    interface Elastos.Droid.Content.IIntent;
    interface Elastos.Droid.App.IPendingIntent;
    interface Elastos.Droid.App.IAlarmManagerAlarmClockInfo;

    namespace Elastos {
