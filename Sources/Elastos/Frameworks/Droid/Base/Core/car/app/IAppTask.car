            [in] String callingPackage,
            [in] IIntent* intent,
            [in] String resolvedType,
            [in] IBundle* options,
            [out] Int32* result);

        SetExcludeFromRecents(
            [in] Boolean exclude);
    }

    } // namespace App
    } // namespace Droid
    } // namespace Elastos
}

