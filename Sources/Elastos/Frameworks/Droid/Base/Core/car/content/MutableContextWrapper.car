
module
{
    interface Elastos.Droid.Content.IContext;
    interface Elastos.Droid.Content.IContextWrapper;

    namespace Elastos {
    namespace Droid {
    namespace Content {

    interface IMutableContextWrapper extends IContextWrapper {
        /**
         * Change the base context for this ContextWrapper. All calls will then be
         * delegated to the base context.  Unlike ContextWrapper, the base context
         * can be changed even after one is already set.
         * 
         * @param base The new base context for this wrapper.
         */
        SetBaseContext(
            [in] IContext* base);
    }

    } // namespace Content
    } // namespace Droid
    } // namespace Elastos
}