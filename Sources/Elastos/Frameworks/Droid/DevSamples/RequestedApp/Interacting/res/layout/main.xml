<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="center"
        android:textColor="#000000"
        android:layout_weight="1"
        android:id="@+id/showntext"
        android:text="Interacting">
    </TextView>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="50dip"
        android:orientation="horizontal" >
        <Button
            android:id="@+id/startotherapp"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:gravity="center"
            android:layout_weight="1"
            android:textColor="#000000"
            android:text="StartWeb">
        </Button>
        <Button
            android:id="@+id/startforresult"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:gravity="center"
            android:textColor="#000000"
            android:layout_weight="1"
            android:text="Start For Result">
        </Button>
        <Button
            android:id="@+id/startanotherapp"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:gravity="center"
            android:textColor="#000000"
            android:layout_weight="1"
            android:text="Start Other app(need ActivityTest app)">
        </Button>
    </LinearLayout>
</LinearLayout>
